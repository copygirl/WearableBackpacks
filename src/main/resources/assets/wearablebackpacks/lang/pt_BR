container.wearablebackpacks.backpack=Mochila

item.wearablebackpacks.backpack.name=Mochila

tooltip.wearablebackpacks.moreInfo=Segure %1$s para mais informações
tooltip.wearablebackpacks.openHint=Pressione %1$s para abrir quando equipado
tooltip.wearablebackpacks.equipHint=Colocar no chão e quebrar\nenquanto esgueirando para equipar.
tooltip.wearablebackpacks.equipHint.extended=Esqueira e quebrar para equipar.\nEsgueirar e clicar com o direito no chão\ncom a mão vazia para equipar.
tooltip.wearablebackpacks.unequipHint=Esgueirar e clicar com o direito no chão\ncom a mão vazia para desequipar.
tooltip.wearablebackpacks.used=Atualmente sendo usado por um jogador

chat.wearablebackpacks.cantEquip=você já tem uma mochila equipada
chat.wearablebackpacks.cantEquip.chestplate=Você não pode equipar uma mochila enquanto usa um peitoral

key.wearablebackpacks.open=Abrir Mochila


config.wearablebackpacks.general.equipAsChestArmor=Equipar como uma Armadura de Peito
config.wearablebackpacks.general.equipAsChestArmor.tooltip=Controla se mochilas equipadas ocupam o slot de equipamento de armadra de peito
config.wearablebackpacks.general.enableEquippedInteraction=Habilitar Interação quando Equipada
config.wearablebackpacks.general.enableEquippedInteraction.tooltip=Controla se jogadores podem acessar mochilas equipadas em outros jogadores (e entidades)
config.wearablebackpacks.general.enableSelfInteraction=Habilitar Auto-Interação
config.wearablebackpacks.general.enableSelfInteraction.tooltip=Controla se jogadores podem acessar sua própria mochila equipada usando uma tecla atalho
config.wearablebackpacks.general.dropAsBlockOnDeath=Soltar como Bloco na Morte
config.wearablebackpacks.general.dropAsBlockOnDeath.tooltip=Controla se mochilas colocam-se após a morte, em vez de espalhar seu conteúdo

config.wearablebackpacks.category.backpack=Configurações da Mochila
config.wearablebackpacks.category.backpack.tooltip=Controla configurações das mochilas regulares
config.wearablebackpacks.backpack.enabled=Habilitado
config.wearablebackpacks.backpack.enabled.tooltip=Controla se a mochila regular está habilitada
config.wearablebackpacks.backpack.durability=Durabilidade
config.wearablebackpacks.backpack.durability.tooltip=Controla a durabilidade da mochila regular (0 para inquebrável). Diminuir isso (incluindo configurar para 0) pode causar problemas com mochilas já danificadas
config.wearablebackpacks.backpack.durability.hint=Sem efeito se não estiver equipada como armadura
config.wearablebackpacks.backpack.armor=Armadura
config.wearablebackpacks.backpack.armor.tooltip=Pontos de armadura da mochilas regulares.
config.wearablebackpacks.backpack.armor.hint=Sem efeito se não estiver equipada como armadura
config.wearablebackpacks.backpack.size=Tamanho do Armazenamento
config.wearablebackpacks.backpack.size.tooltip=Controla o tamanho do armazenamento das mochilas regulares. Não afeta mochilas colocas ou equipadas
